{% extends "FOSUserBundle::layout.html.twig" %}

{% block fos_user_content %}
    <div class="container">

		<div class="row">
			<div class="col-md-6">
				<h1>Message pub</h1>
			</div>
	    	
			<div class="col-md-6">
				Entreprise
			    {% trans_default_domain 'FOSUserBundle' %}

			    {{ form_start(form, {'method': 'post', 'attr': {'class': 'entreprise_registration'}}) }}
			        {{ form_widget(form) }}
			        <div>
			            <input type="submit" value="{{ 'registration.submit'|trans }}" />
			        </div>
			    {{ form_end(form) }}
			</div>
		</div>

    </div>
{% endblock fos_user_content %}